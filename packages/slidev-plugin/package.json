{
  "name": "@slidev-ai/slidev-plugin",
  "version": "0.1.0",
  "description": "Slidev plugin for AI-powered slide editing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/client/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/client/index.ts --format esm --dts --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@slidev-ai/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vue": "^3.5.25"
  },
  "peerDependencies": {
    "@slidev/cli": ">=0.47.0",
    "vue": "^3.3.0"
  }
}
